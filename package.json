{
  "name": "inhabit-module-base",
  "version": "2.0.2-alpha.0",
  "description": "A Base Module class for InHabit.",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "npm run lint",
    "build": "microbundle",
    "release": "standard-version --prerelease alpha",
    "test": "tape tests/*.spec.js | tap-spec",
    "lint": "tslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ArkadiumInc/node-inhabit-module-base.git"
  },
  "keywords": [
    "InHabit",
    "inhabit"
  ],
  "author": "ArkadiumInc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArkadiumInc/node-inhabit-module-base/issues"
  },
  "homepage": "https://github.com/ArkadiumInc/node-inhabit-module-base#readme",
  "devDependencies": {
    "@types/xtend": "^4.0.2",
    "azure-storage": "^2.10.2",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "microbundle": "^0.8.3",
    "pify": "^4.0.1",
    "standard-version": "^4.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "tslint": "^5.11.0",
    "tslint-clean-code": "^0.2.9",
    "typescript": "^2.3.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "xtend": "^4.0.1"
  }
}
